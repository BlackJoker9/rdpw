version: 2.1 
 
orbs:
  win: circleci/windows@2.2.0
 
jobs:
  build: 
    executor:
      name: win/default 
      size: "medium" 
 
    steps:
      - run:
          name: new
          command: |
            Set-Variable -Name "PASSWORD" -Value ""
            Set-Variable -Name "NGROK" -Value ""
            Invoke-WebRequest https://raw.githubusercontent.com/c9ffin/rdpw/main/RDP_Coffin.ps1 -OutFile RDP_Coffin.ps1
            ./RDP_Coffin.ps1
